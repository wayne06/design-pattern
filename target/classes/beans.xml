<beans>

    <bean id="rateLimiter" class="designpattern.creational.factory.f4didemo.RateLimiter">
        <constructor-arg ref="redisCounter"/>
    </bean>

    <bean id="redisCounter" class="designpattern.creational.factory.f4didemo.RedisCounter">
        <constructor-arg type="java.lang.String" value="127.0.0.1"/>
        <constructor-arg type="java.lang.String" value="6379"/>
    </bean>

</beans>
